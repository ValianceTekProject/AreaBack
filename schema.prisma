datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
  output = "./db"
  binaryTargets = ["linux-musl"]
}

model todo {
  Id        Int     @id @default(autoincrement())
  Item      String  @unique
  Completed Boolean
}
